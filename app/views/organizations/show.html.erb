<p id="notice"><%= notice %></p>

<p>
  <b>Organization Name:</b>
  <%= @organization.name %>
</p>

<p>
  <b>Created by User:</b>
  <%= @organization.user_id %>
</p>

<h1>Users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
  </tr>
<% end %>
</table>

<br />


<%= link_to 'Edit', edit_organization_path(@organization) %>