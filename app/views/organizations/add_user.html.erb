<h1>Add User to Organization</h1>

<p>
  <b>Organization Name:</b> <%= @organization.name %>
</p>

<%= form_for(@organization) do |f| %>
  <p class="field">
    <%= select("params", "user_id"  , User.all.collect {|u| [ u.name, u.id ] }, {:prompt => 'Select User'}) %>
  </p>

  <p class="actions">
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Show', @organization %>
